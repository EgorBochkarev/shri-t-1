{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "google",
        "plugin:react/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "ignorePatterns": ["dist/", "node_modules/"],
    "rules": {
        "require-jsdoc": ["off"],
        "comma-dangle": ["off"],
        "react/prop-types": ["off"]
    }
}